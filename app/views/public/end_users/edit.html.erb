<div class="container">
  <div class="row">
    <div class="col-xs-2"></div>
    <div class="col-xs-3">
      <h3>登録情報編集</h3>
    </div>
  </div>
  <div class="row" style="margin: 10px 0 0 0;">
    <%= form_for(@end_user, url: end_users_path) do |f| %>

              <div class="form-group">
                <div class="row">
                  <div class="col-xs-3"><label for="InputEmail">名前</label></div>
                  <div class="col-xs-3"><%= f.text_field :last_name, autofocus: true, class:"form-control"%></div>
                  <div class="col-xs-3"><%= f.text_field :first_name, autofocus: true, class:"form-control"%></div>
                </div>
              </div>

              <div class="form-group">
                <div class="row">
                  <div class="col-xs-3"><label for="InputEmail">フリガナ</label></div>
                  <div class="col-xs-3"><%= f.text_field :last_name_kana, autofocus: true, class:"form-control"%></div>
                  <div class="col-xs-3"><%= f.text_field :first_name_kana, autofocus: true, class:"form-control"%></div>
                </div>
              </div>

              <div class="form-group">
                <div class="row">
                  <div class="col-xs-3"><label for="InputEmail">メールアドレス</label></div>
                  <div class="col-xs-3"><%= f.text_field :email, autofocus: true, class:"form-control", id:"inputEmail", placeholder:"名前"%></div>
                </div>
              </div>

              <div class="form-group">
                <div class="row">
                  <div class="col-xs-3"><label for="InputEmail">郵便番号(ハイフンなし)</label></div>
                  <div class="col-xs-3"><%= f.text_field :postcode, autofocus: true, class:"form-control"%></div>
                </div>
              </div>

              <div class="form-group">
                <div class="row">
                  <div class="col-xs-3"><label for="InputEmail">住所</label></div>
                  <div class="col-xs-6"><%= f.text_field :user_address, autofocus: true, class:"form-control"%></div>
                </div>
              </div>

              <div class="form-group">
                <div class="row">
                  <div class="col-xs-3"><label for="InputEmail">電話番号(ハイフンなし)</label></div>
                  <div class="col-xs-3"><%= f.text_field :phone_number, autofocus: true, class:"form-control"%></div>
                </div>
              </div>

             <div class="actions" style="">
              <div class="row">
                <div class="col-xs-3">
                </div>
                <div class="col-xs-5">
                  <%= f.submit "編集内容を保存する", class:"btn btn-primary" %>
                </div>
                <div class="col-xs-3">
                  <%= link_to ' 退会する', end_users_withdrow_path, class: 'btn btn-danger' %>
                </div>
              </div>
             </div>
          <% end %>

  </div>
</div>